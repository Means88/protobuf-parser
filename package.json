{
  "name": "protobuf-parser",
  "version": "0.1.0",
  "description": "proto buffer parser",
  "main": "index.js",
  "scripts": {
    "build": "rimraf dist && tsc --build tsconfig.build.json",
    "lint": "run-s lint:prettier lint:tslint",
    "lint:prettier": "prettier-check '{src,test}/**/*.ts'",
    "lint:tslint": "tslint -t stylish -p .",
    "fix": "run-s fix:tslint fix:prettier",
    "fix:prettier": "prettier --write '{src,test}/**/*.ts'",
    "fix:tslint": "tslint --fix -t stylish -p .",
    "test": "jest",
    "test:watch": "jest --watch --notify"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.test.ts": [
      "yarn prettier --write",
      "git add"
    ],
    "*.ts": [
      "yarn fix:tslint",
      "yarn prettier --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Means88/protobuf-parser.git"
  },
  "keywords": [
    "protobuf",
    "parser"
  ],
  "author": "Means88",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Means88/protobuf-parser/issues"
  },
  "homepage": "https://github.com/Means88/protobuf-parser#readme",
  "devDependencies": {
    "@types/chai": "^4.1.6",
    "@types/jest": "^23.3.7",
    "@types/node": "^10.12.0",
    "@types/pegjs": "^0.10.1",
    "@types/prettier": "^1.13.2",
    "@types/sinon": "^5.0.5",
    "chai": "^4.2.0",
    "husky": "^1.1.2",
    "jest": "^23.6.0",
    "lint-staged": "^8.0.3",
    "npm-run-all": "^4.1.3",
    "prettier-check": "^2.0.0",
    "rimraf": "^2.6.2",
    "sinon": "^7.1.0",
    "ts-jest": "^23.10.4",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "typescript": "^3.1.3"
  },
  "dependencies": {
    "execute-me": "^0.2.0",
    "pegjs": "^0.10.0",
    "prettier": "^1.15.2"
  },
  "jest": {
    "collectCoverage": true,
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "testEnvironment": "node",
    "testMatch": [
      "**/*.test.ts"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": "./tsconfig.json"
      }
    }
  }
}
